{
  "name": "odin-validator",
  "version": "1.0.3",
  "description": "A lib that helps you validate params in a blink of eye",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "author": "Marcos Soares <marcossaore@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "jestSetup": "jest --passWithNoTests --silent --noStackTrace --runInBand --bail",
    "test": "yarn jestSetup --watch",
    "test:staged": "yarn jestSetup --findRelatedTests",
    "test:ci": "yarn jestSetup --coverage",
    "test:verbose": "jest --passWithNoTests --runInBand",
    "build": "rimraf dist && tsc -p tsconfig-build.json",
    "prepublish": "yarn build"
  },
  "devDependencies": {
    "@types/jest": "^26.0.24",
    "@types/faker": "^5.5.7",
    "@typescript-eslint/eslint-plugin": "^4.28.5",
    "eslint": "^7.32.0",
    "eslint-config-standard-with-typescript": "^20.0.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "git-commit-msg-linter": "^3.2.6",
    "jest": "^27.0.6",
    "jest-mock-extended": "^1.0.18",
    "lint-staged": "^11.0.0",
    "faker": "^5.5.3",
    "ts-jest": "^27.0.4",
    "typescript": "^4.3.5",
    "rimraf": "^3.0.2"
  },
  "directories": {
    "test": "tests"
